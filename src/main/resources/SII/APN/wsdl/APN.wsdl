<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:srv="http://www.sii.acquirenteunico.it/APN"
	xmlns:msgSII="http://www.acquirenteunico.it/schemas/2010/SII_AU/MessaggioSII"
	targetNamespace="http://www.sii.acquirenteunico.it/APN">


	<wsdl:types>
		<xs:schema xmlns="http://www.w3.org/2001/XMLSchema">
			<xs:import namespace="http://www.acquirenteunico.it/schemas/2010/SII_AU/MessaggioSII"
				schemaLocation="../schemas/APN_msgSII.xsd"/>
		</xs:schema>
	</wsdl:types>

	<!-- *** Messages *** -->

	<wsdl:message name="ComunicazioneAnnullamentoPraticaMessaggioSII">
		<wsdl:part name="MessaggioSII" type="msgSII:ComunicazioneAnnullamentoPraticaMessaggioSIIType"/>
	</wsdl:message>

	<wsdl:message name="DefaultResponseMessaggioSII">
		<wsdl:part name="MessaggioSII" type="msgSII:DefaultResponseMessaggioSIIType"/>
	</wsdl:message>

	<!-- *** PortTypes *** -->

	<wsdl:portType name="APN">
		
		<wsdl:operation name="APN.0200">
			<wsdl:input message="srv:ComunicazioneAnnullamentoPraticaMessaggioSII"/>
			<wsdl:output message="srv:DefaultResponseMessaggioSII"/>
		</wsdl:operation>

	</wsdl:portType>

	<!-- *** Bindings *** -->

	<wsdl:binding name="APNBinding" type="srv:APN">

		<soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
		
		<wsdl:operation name="APN.0200">
			<soap:operation soapAction="APN.0200"/>
			<wsdl:input>
				<soap:body
					namespace="http://www.sii.acquirenteunico.it/APN"
					parts="MessaggioSII" use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body
					namespace="http://www.sii.acquirenteunico.it/APN"
					parts="MessaggioSII" use="literal"/>
			</wsdl:output>
		</wsdl:operation>

	</wsdl:binding>


	<!-- *** Services *** -->

	<wsdl:service name="APNService">
		<wsdl:port name="APNPort" binding="srv:APNBinding">
			<soap:address location="http://localhost:8080/APN"/>
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>